<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.software_development_project"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    tools:node="merge">

    <dist:module dist:instant="true" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="whateverString"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Software_Development_Project"
        tools:replace="android:appComponentFactory"
        tools:ignore="AllowBackup"
        tools:targetApi="p">
        <activity
            android:name="com.example.software_development_project.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.Software_Development_Project.NoActionBar"></activity>
        <activity android:name="com.example.software_development_project.AdminCategory" />
        <activity android:name="com.example.software_development_project.AdminProductActivity" />
        <activity android:name="com.example.software_development_project.RegisterActivity" />
        <activity android:name="com.example.software_development_project.LoginActivity" />
        <activity android:name="com.example.software_development_project.LogChoice" />
        <activity android:name="com.example.software_development_project.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
